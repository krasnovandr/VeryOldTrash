<div class="container">
    <div class="hero-unit">
        <div id="Container"></div>
    </div>
</div>
<script type="text/template" id="calculatorTemplate">
    <div class="container tblCalc" >
      <div class="row">
            
                <input type="text" class="text-right" value="<%= current %>" disabled="disabled" /><input type="text"  value="<%= dataInMem %>" style="width:20px" disabled="disabled"  />
                
     
       
        </div>
       <div class="row">
            <button data-rel="11" class="operator btn span1">MR</button>
            <button data-rel="12" class="operator btn span1">MS</button>
            <button data-rel="12" class="operator btn span1">MC</button>
            <button data-rel="13" class="operator btn span1">M+</button>  
            <button data-rel="13" class="operator btn span1">M-</button>  
         </div> 
        <div class="row">
            <button data-rel="1" class="digit btn span1">1</button>
            <button data-rel="2" class="digit btn span1">2</button>
            <button data-rel="3" class="digit btn span1">3</button>
        </div>
        <div class="row">
        
            <button data-rel="4" class="digit btn span1">4</button>
            <button data-rel="5" class="digit btn span1">5</button>
            <button data-rel="6" class="digit btn span1">6</button>
        
        </div>
        <div class="row">
            <button data-rel="7" class="digit btn span1">7</button>
            <button data-rel="8" class="digit btn span1">8</button>
            <button data-rel="9" class="digit btn span1">9</button>
            
     </div>
          <div class="row">
            <button data-rel="0" class="digit btn span1">0</button>        
            <button data-rel="10" class="operator btn span1">C</button>
          </div>
    </div>

</script>

<script>
    $(function () {
        View.run();
    });
</script>
